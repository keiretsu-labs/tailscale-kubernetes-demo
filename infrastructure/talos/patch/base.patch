cluster:
  discovery:
    enabled: true
  allowSchedulingOnControlPlanes: true
  network:
    cni:
      name: none
    dnsDomain: cluster.robbinsdale
    podSubnets:
    - 10.1.0.0/16
    serviceSubnets:
    - 10.0.0.0/16
  proxy:
    disabled: true
  controlPlane:
    endpoint: https://k8s.robbinsdale.local:6443
  clusterName: k8s.robbinsdale.local
  apiServer:
    certSANs:
    - k8s.robbinsdale.local
    - k8s.robbinsdale.rajsingh.info
    - k8s.robbinsdale.lukehouge.com
machine:
  features:
    hostDNS:
      enabled: false # Enable host DNS caching resolver.
      forwardKubeDNSToHost: false
  certSANs:
  - k8s.robbinsdale.local
  - k8s.robbinsdale.rajsingh.info
  - k8s.robbinsdale.lukehouge.com
  sysctls:
    vm.nr_hugepages: "1024"
  nodeLabels:
    openebs.io/engine: mayastor
  kubelet:
    extraArgs:
      rotate-server-certificates: true
    extraMounts:
    - destination: /var/openebs/local
      type: bind
      source: /var/openebs/local
      options:
      - rbind
      - rshared
      - rw